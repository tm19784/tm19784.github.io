{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Bitcoin price from DATE",
    "subtitle": "Daily API values converted into raw data using Python loop",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/tm19784/tm19784.github.io/main/New%20COVID%20cases%20live%20data.csv",
    "format": {"type": "json"}
  },
  
  "height": 300,
  "width": 300,
  "mark": {"type": "line", "point": false},
  "encoding": {
    "x": {
      "field": "INSERT",
      "type": "temporal",
      "title": null,
      "axis": {"grid": true}
    },
    "y": {
      "field": "INSERT",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": false}
    }
  },
  "config": {}
}
